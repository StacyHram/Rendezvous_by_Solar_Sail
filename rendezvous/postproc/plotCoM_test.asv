function figureCoM = plotCoM_test( simulationResults, simulationResults_test)

elements = double.empty(6, 0); %elements on all orbit
 for i=1:1:(size(hp))
     hp(i, 1:6);
     [p, a, ecc, incl, omega, argp, nu, ~, ~, ~, ~] = rv2coe(hp(i,1:3) * m2km, hp(i, 4:6) * m2km);
     elements(i,1) = a; elements(i,2) = ecc; elements(i, 3) = incl;
     elements(i, 4) = omega; elements(i, 5) = argp; elements(i, 6) = nu;
 end
figureCoM = figure;
plot( simulationResults.x, simulationResults.y(3, :), '-b');
hold on
plot( simulationResults_test.x, simulationResults_test.y(3, :), '-r'); 

end